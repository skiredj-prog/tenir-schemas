{
  "$id": "https://USERNAME.github.io/tenir-schemas/schemas/v1/time-window.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TimeWindow",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "unit",
    "value"
  ],
  "properties": {
    "unit": {
      "type": "string",
      "enum": [
        "DAYS",
        "WEEKS",
        "MONTHS",
        "YEARS"
      ]
    },
    "value": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1200
    }
  }
}