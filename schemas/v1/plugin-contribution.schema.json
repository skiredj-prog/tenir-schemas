  "properties": {
    "plugin_id": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "added_nodes": {
      "type": "array",
      "items": {
        "$ref": "trace-node.schema.json"
      },
      "default": []
    },
    "added_outputs": {
      "type": "object",
      "additionalProperties": true
    }
  }
}